.add-feed-row
  .col-4
    input(type='text', placeholder='Name RSS Feed')
  .col-6
    input(type='text', placeholder='Url RSS Feed')
  .col-2
    a.btn(href='')
      | Add Feed
hr
rss-feed(list="array")
div(manage-rss-list)
  div(ng-repeat='item in hc.rssFirst')
    h2
      | {{item.author}}
    div(ng-bind-html='item.content')
    p
      | {{item.publishedDate}}
    a(ng-href='{{item.link}}')
      | {{item.title}}
    hr
